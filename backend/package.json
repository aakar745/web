{
  "name": "web-tools-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "worker": "node worker.js",
    "dev:worker": "nodemon --exec \"node worker.js\"",
    "start:all": "concurrently \"npm run dev\" \"npm run dev:worker\"",
    "indexes": "npx ts-node src/scripts/createIndexes.ts",
    "indexes:create": "npx ts-node src/scripts/createIndexes.ts create",
    "indexes:analyze": "npx ts-node src/scripts/createIndexes.ts analyze",
    "indexes:cleanup": "npx ts-node src/scripts/createIndexes.ts cleanup",
    "db:check-sharding": "npx ts-node src/scripts/checkSharding.ts",
    "indexes:build": "npx tsc src/scripts/createIndexes.ts --outDir dist/scripts --moduleResolution node --esModuleInterop",
    "indexes:create-prod": "npm run build && node dist/scripts/createIndexes.js create",
    "indexes:analyze-prod": "npm run build && node dist/scripts/createIndexes.js analyze",
    "db:check-sharding-prod": "npm run build && node dist/scripts/checkSharding.js"
  },
  "dependencies": {
    "@types/archiver": "^6.0.3",
    "@types/opossum": "^8.1.8",
    "archiver": "^7.0.1",
    "axios": "^1.9.0",
    "bcrypt": "^6.0.0",
    "bull": "^4.12.8",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.3.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.15.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "opossum": "^8.4.0",
    "redis": "^4.6.15",
    "sharp": "^0.33.1",
    "uuid": "^11.1.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bull": "^3.15.9",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/mongoose": "^5.11.96",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.0.0",
    "@types/sharp": "^0.31.1",
    "@types/uuid": "^10.0.0",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
